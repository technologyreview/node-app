##
# Clustering for Node.js to AWS Elastic Beanstalk 
#
# http://docs.aws.amazon.com/elasticbeanstalk/latest/dg/create_deploy_nodejs_express_elasticache.html
##
option_settings:
  "aws:elasticbeanstalk:customoption" :
     CacheNodeType : cache.t2.micro
     NumCacheNodes : 1
     Engine : redis
     NodeListPath : /var/nodelist